{
  "name": "playwright-flakerfinder",
  "version": "1.0.0",
  "type": "module",
  "description": "Demonstrates techniques for finding and addressing test flakiness",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:local-headed": "playwright test --project=chrome --headed",
    "test:local-headless": "playwright test --project=chrome",
    "test:local-flake": "playwright test --project=chrome-for-flake",
    "test:ci": "playwright test --project=chrome-for-flake",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:compare": "node scripts/performance-comparison.js",
    "install": "playwright install",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "playwright",
    "testing",
    "flakiness",
    "e2e",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-plugin-playwright": "^2.2.2",
    "prettier": "^3.6.2",
    "typescript": "^5.0.0"
  }
}
